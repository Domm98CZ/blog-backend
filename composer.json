{
    "name": "domm98cz/blog-backend",
    "description": "Sample PHP+Nette backend of blog",
    "keywords": ["nette"],
    "type": "project",
    "require": {
        "nette/application": "^3.2",
        "nette/bootstrap": "^3.2",
        "nette/database": "^3.2",
        "nette/di": "^3.2",
        "nette/http": "^3.3",
        "nette/utils": "^4.0",
        "nette/neon": "^3.4",
        "nette/security": "^3.2",
        "latte/latte": "^3.0",
        "nextras/orm": "^5.0",
        "nextras/migrations": "^3.4",
        "contributte/console": "^0.10.1",
        "contributte/apitte": "^0.12.3",
        "contributte/di": "^0.5.6",
        "domm98cz/curl": "^0.4.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app",
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "tests": "vendor/bin/phpunit"
    },
    "minimum-stability": "stable",
    "require-dev": {
        "tracy/tracy": "^2.10",
        "phpunit/phpunit": "^12.0",
        "mockery/mockery": "^1.6"
    }
}
